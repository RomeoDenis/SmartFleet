@using SmartFleet.Resources
<div class="form-group">
    <div id="start_datetime" style="width: 250px; margin-top: 5px;display: none" class="input-group input-append date ">
       
        <input  class=" form-control" type="text" id="start-period" size="16" value="" >
        <span class="add-on input-group-addon">
            <i class="fa fa-calendar"></i>
        </span>
    </div>
</div>
<div class="form-group">
    <div style="width: 250px; display: none;margin-top: -9px;" class="input-group date " id="end_datetime">

        <input class=" form-control" type="text" id="end-period" size="16">
        <span class="add-on input-group-addon">
            <i class="fa fa-calendar"></i>
        </span>
    </div>
</div>
<div class="form-group">
    <div style="width: 250px" class="input-group date "  >
        <button onclick="downloadReport()" style="display: none" id="btn-dwnl" class="btn btn-default">@Resource.Download</button>
    </div>
</div>